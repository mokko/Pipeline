	<xsd:annotation>
		<xsd:documentation>
		Todo
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="kALuftfeuchtigkeit" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
		Todo
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="kATemperatur" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
		Todo
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="kurzeBeschreibung" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Freitextfeld zur kurzen Beschreibung/Zusammenfassung des Inhalts des Objekts.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="lagerungSicherheitPräsentation" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			TODO
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="konvolut" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Bezeichnung eines Konvoluts, zu dem dieses Objekt gehört.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="langeBeschreibung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Freitextfeld zur langen Beschreibung/Zusammenfassung des
    Inhalts des
    Objekts.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="leihgeber" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Person oder Körperschaft, die Gegenstand an EMEM
    verleiht.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="maßangaben">
 <xsd:annotation>
  <xsd:documentation> beschreibt die Maßangaben des Gegenstandes und damit
    zusammenhängende Informationen.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="typ" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="materialTechnik">
 <xsd:annotation>
  <xsd:documentation> Angabe zum Material des Gegenstandes oder damit
    zusammenhängende
    Techniken.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="besonderheit" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

 <!-- Zusatz: Audio/Film? -->
<xsd:element name="musikgattung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>Musikalische Gattung.</xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="nadelschliff" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>Nadel, die zur Herstellung einer Kopie verwendet
    wurde.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="objektgruppe" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>Name einer Objektgruppe, zu der das Objekt gehört.</xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="objekttyp" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Bestimmt, welchem Bereich/Typen das Objekt unterzuordnen
    ist. bspw.
    'audio' (für Schallplatte), 'Freitextfeld zur kurzen Beschreibung/Zusammenfassung
    des Inhalts des Objekts, z.B. "Audio" (Schallplatte), "Film/Video",
    "Druckgrafik",
    "Fotografie" etc.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="objStatus" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>missing.</xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="onlineBeschreibung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>missing.</xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="oov">
 <xsd:annotation>
  <xsd:documentation> Objekt-Objekt-Verknüpfung (Umschlag). Das Element
    verweist auf die
    Ident.Nr eines verknüpften Objekts. Das Attribut "art" charakterisiert die Art der
    Verknüpfung, das Attribut "bemerkung" beschreibt die Verknüpfung weiter.
    Attribut id
    trägt die objId des verknüpften Objekts.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="bemerkung" type="xsd:string"/>
    <xsd:attribute name="id" type="xsd:string"/>
    <xsd:attribute name="sachbegriff" type="xsd:string"/>
    <xsd:attribute name="titel" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

 <!-- 
 	Achtung: aktuell ist der doppelte Singuar: person nicht personen etc. 

	CURRENTLY NOT USED 	
-->
<xsd:element name="personKörperschaftRef">
 <xsd:annotation>
  <xsd:documentation> Bestimmt eine Person oder Körperschaft, die mit dem
    Objekt
    assoziiert wird, bspw. den Namen des Herstellers oder der Plattenfirma einer
    Tonaufnahme.
   </xsd:documentation>
  </xsd:annotation>

 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="id" type="xsd:string"/>
    <xsd:attribute name="funktion" type="xsd:string"/>

    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="sachbegriff">
 <xsd:annotation>
  <xsd:documentation> Bestimmt das Objekt als Medium, bspw. "CD",
    "Schallplatte", "Dia",
    "DVD", "Kassette" etc.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="sachbegriffHierarchisch">
 <xsd:annotation>
  <xsd:documentation> Sachbegriff mit allen Begriffen in der Hierarchie der 
  Systematik. Z.B. Tongefäß (04. Wohnkultur, Architektur / Hausrat / 
  Nutzgegenstände / Aufbewahren / Behälter und Gefäße (allgemein) / 
  allgemeine Gefäße und Zubehör)
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="ständigerStandort" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Ständiger Standort des Gegenstandes innerhalb der
    Institution.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="swd">
 <xsd:annotation>
  <xsd:documentation> Verschlagwortung des Objekts mit SWD-Begriffen
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="systematikArt" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Bestimmt durch eine bestimmte Systematik die Art des
    Mediums und den
    Kontext, in welchem das Objekt entstand, bspw. "Dokumentation", "Feldforschung",
    "Gruppenportrait", "Reisegrafie",
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

 <!-- Zusatz: Audio -->
<xsd:element name="technischeBemerkung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Freitextfeld für technische Bemerkungen zum Objekt.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

 <!-- Zusatz: Audio -->
<xsd:element name="textOriginal" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Freitextfeld, das den Originaltext des Objekts
    beinhaltet, bspw. den
    Text eines Liedes.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="titel">
 <xsd:annotation>
  <xsd:documentation> Freitextfeld, das den Titel des Objekts beinhaltet, bspw.
    die
    Aufschrift eines Plattencovers.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="bemerkung" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="ton" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Angaben zu Kanaligkeit von Filmen (Format, Film,
    veraltetet).
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="tvNorm" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> TV-Norm (Format, Film, veraltetet). </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="verantwortlich">
	<xsd:annotation>
		<xsd:documentation> Bei Sammlungsobjekten und PersonenKörperschaften:
		Organisatorische
		Einheit (z.B. Abteilung), die für den Datensatz und das durch ihn beschriebene
		Objekt bzw. PersonKörperschaft verantwortlich ist.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AKu-Ostasiatische Kunst"/>
			<xsd:enumeration value="AKu-Süd-, Südost- und Zentralasien"/>
			<xsd:enumeration value="EM-Afrika"/>
			<xsd:enumeration value="EM-Allgemein"/>
			<xsd:enumeration value="EM-Am Archäologie"/>
			<xsd:enumeration value="EM-Am Ethnologie"/>
			<xsd:enumeration value="EM-Archiv"/>
			<xsd:enumeration value="EM-Islamischer Orient"/>
			<xsd:enumeration value="EM-Ost- und Nordasien"/>
			<xsd:enumeration value="EM-Phonogramm-Archiv"/>
			<xsd:enumeration value="EM-Medienarchiv"/>
			<xsd:enumeration value="EM-Musikethnologie"/>
			<xsd:enumeration value="EM-Neuerwerbung"/>
			<xsd:enumeration value="EM-Süd- und Südostasien"/>
			<xsd:enumeration value="EM-Südsee/Australien"/>
			<xsd:enumeration value="Musterdatensätze Sammlung"/>
			<xsd:enumeration value="SPK-Personen/Körperschaften"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:element>

<xsd:element name="verfügbareFormate" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Formate, in denen dieser Film verfügbar ist.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="verwaltendeInstitution" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Definiert die Institution, die das Objekt verwaltet und
    in dessen
    Inventarliste das Objekt zu finden ist.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="vorlage" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Beschreibt das Ausgangsmedium der
    Standard-Digitalisierung dieses
    Mediums (z.B. ob Walzen-Positiv oder Negativ). Audio-Zusatz 2.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="wGAusVorgaben" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation></xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="wGGruppe" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation></xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="wGRestzeitGh" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation></xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="wGStänderung" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation></xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="wGZustand" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation></xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="zielgruppe" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Zielgruppe, auf die der Film ausgerichtet ist.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>
</xsd:schema>
