<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 targetNamespace="http://www.mpx.org/mpx" xmlns="http://www.mpx.org/mpx"
 elementFormDefault="unqualified">

 <!--
	20200211: fresh start for mpx version 2.0
 -->

<xsd:element name="museumPlusExport">
	<xsd:annotation>
		<xsd:documentation>
			Root Element: Container für MuseumPlus (M+) Datensätze.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element ref="multimediaobjekt" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="personKörperschaft" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="sammlungsobjekt" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="level" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
				Level can be "clean" or "dirty". Clean is proper mpx, dirty is the matrix representation,
				an intermediate step in the creation of an mpx file.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="version" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
				MPX Version. Introduced in version 2.0
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
</xsd:element>


<!-- CLASS 1 Multimediaobjekte -->

<xsd:element name="multimediaobjekt">
	<xsd:annotation>
		<xsd:documentation> 
		Container für Multimedia-Datensätze. Das Attribut "mulId" enthält einen
		eindeutigen Index für jeden Datensatz.
		Das Attribut "sort" ist ein Integer und sortiert bei Bedarf weitere Medien nach einer
		Reihenfolge, wobei 0 immer das bevorzugte Medium ist. Wenn kein sort , sind alle Medien gleichbereichtigt.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element ref="anfertDat" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="bearbDatum" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="bemerkFoto" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="dateiname" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="erweiterung" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="farbe" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="format" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="fotoNegNr" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="funktion" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="inhaltAnsicht" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="matTechn" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="personenKörperschaften" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="pfadangabe" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="standardbild" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="standort" minOccurs="0" maxOccurs="1"/>
			<!-- currently not in alphabetical order! -->
			<xsd:element ref="typ" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="vervielfVon" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="urhebFotograf" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="verknüpftesObjekt" minOccurs="0" maxOccurs="1"/>
            <xsd:element ref="veröffentlichen" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="exportdatum" type="xsd:dateTime"/>
		<xsd:attribute name="mulId" type="xsd:integer"/>
	</xsd:complexType>
</xsd:element>

<!-- main level -->
<xsd:element name="anfertDat" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Anfertigungsdatum des Multimedia-Objekts.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="bemerkFoto" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>missing.</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="dateiname" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Ort der Multimedia-Objekts auf der lokalen Festplatte (Dateiname).
			SN: wird komplementiert durch den Multimedia.Pfadangabe.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="erweiterung" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>Dateiendung, z.B. jpg</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="farbe" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
		mm/farbe: die Farbgebung des Multimedia-Objekts. 
		Zusatz1/farbe: Farbformat des Films

		SN: nur bei Fotos und ähnlichen Objekten. SN: "S/W" (für schwarz/weiß)
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="format" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
		mm/format: beschreibt die Maße des Objekts. SN: z.B. Länge, Breite oder Durchmesser.
		Zusatz 1 Audio: Audio Format des Gegenstandes. 
		
		SN: bezieht sich auf Eigenschaften digitaler Formate, z.B. "sample rate".
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="fotoNegNr" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Nummer des zu einem Foto gehörenden Negativs.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="funktion" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			beschreibt die Verwendung des Multimedia-Objektes.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="inhaltAnsicht" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			inhaltliche Beschreibung der Abbildung oder Beschreibung der Ansicht des Fotos.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
<xsd:element name="matTechn" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation> 
			Nennt wesentliche Materialien und Techniken. 
			SN: Bei Fotos beispielsweise "Acetatfilm", "Polaroidverfahren".
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="personenKörperschaften">
	<xsd:annotation>
		<xsd:documentation>
			in SO and in MM
			Der Elementname sollte im Singular formuliert sein, aber nicht jetzt.
			Todo, aber nicht jetzt
			assozierte Personen/Körperschaften
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="funktion" type="xsd:string"/>
				<xsd:attribute name="art" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:element>

<xsd:element name="pfadangabe" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation> Ort des Multimedia-Objekts auf der lokalen Festplatte
		(Pfad). SN: wird komplementiert durch den Multimedia.Dateiname.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="typ" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation> 
			MM/Typ
			Ort des Multimedia-Objekts auf der lokalen Festplatte (Pfad). 
			
			SN: wird komplementiert durch den Multimedia.Dateiname.
		
			PK.Typ: Bestimmt die Person oder Institution. 

			SN: z.B. als "Person" oder "Körperschaft".
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>
		
<xsd:element name="urhebFotograf" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Urheber und/oder Fotografen.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="veröffentlichen" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Soll das Medium veröffentlicht werden (ja/nein)?
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="vervielfVon" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Todo
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="objId" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Index für ein Sammlungsobjekt-Datensatz.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>


 <!-- CLASS 2 PersonKörperschaft -->

<xsd:element name="personKörperschaft">
 <xsd:annotation>
  <xsd:documentation> Container für M+Datensätze. Das Attribut "kueId" enthält
    einen
    eindeutigen Index für jeden Datensatz in M+.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:sequence>
	   <xsd:element ref="artKörpersch" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="bearbDatum" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="bemerkungen" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="berufTätigkeit" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="biographie" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="datierung" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="geogrBezug" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="kurzbiographie" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="name" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="nationalität" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="nennform" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="quelle" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="titelStand" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="typ" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="verantwortlich" minOccurs="0" maxOccurs="unbounded"/>
	   <xsd:element ref="verknüpftesObjekt" minOccurs="0" maxOccurs="unbounded"/>
   </xsd:sequence>
  <xsd:attribute name="exportdatum" type="xsd:dateTime"/>
  <xsd:attribute name="kueId" type="xsd:integer"/>
  </xsd:complexType>
</xsd:element>

 <!-- main level -->
<xsd:element name="artKörpersch" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> bestimmt die Institution näher. SN: z.B. als
    "Hochschule",
    "Plattenlabel" oder "Behörde".
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="bemerkungen" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Anmerkungen zu der Person oder Institution.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="berufTätigkeit" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> näher bestimmte Beschreibung des Berufs, der
    Beschäftigung oder
    Arbeitsbereich der Person oder der Institution. Jeder darauf bezogene Datensatz
    innerhalb dieses Komplexes wird als "profession record" bezeichnet.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="biographie" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> eine Beschreibung des Lebens der Person oder eine
    Beschreibung der
    Geschichte der Institution. SN: EMEM hat Üblicherweise nur in "Kurzbiographie" gute
    Daten.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="kurzbiographie" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> eine kurze Beschreibung des Lebens der Person
    (Lebenslauf) oder eine
    kurze Beschreibung der Geschichte der Institution.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="name">
 <xsd:annotation>
  <xsd:documentation> Name einer Person/Körperschaft TODO: Überprüfen Attribute
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="nationalität" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Nationalität einer Person/Körperschaft.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="nennform">
 <xsd:annotation>
  <xsd:documentation> Nennform und dazugehörige Informationen
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="quelle" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Informationsquelle für die Person oder Institution. SN:
    Hierbei
    werden für Mehrfacheinträge Freitextfelder verwendet.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="titelStand" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Titel einer Person. SN: Beispiele: "Dr.", "Professor",
    "Pandit". Bei
    Körperschaften Feld nicht verwenden. Mehrfache Titel können sowohl in
    Wiederholfelder geschrieben werden als auch in einem Feld wiederholt werden. Im EM
    wird letzteres bevorzugt.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>


<xsd:element name="standort">
	<xsd:annotation>
		<xsd:documentation>Standort (aus Standortgeschichte)</xsd:documentation>
	</xsd:annotation>
	
	<xsd:complexType>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="status" type="xsd:string"/>
				<xsd:attribute name="art" type="xsd:string"/>
				<xsd:attribute name="detail" type="xsd:string"/>
				<xsd:attribute name="bearbMit" type="xsd:string"/>
				<xsd:attribute name="bearbDat" type="xsd:string"/>
				<xsd:attribute name="datumVon" type="xsd:string"/>
				<xsd:attribute name="datumBis" type="xsd:string"/>
				<xsd:attribute name="kommentar" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:element>

<xsd:element name="standardbild" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Wenn dieses Element vorhanden, dann ist das entsprechende
    multimediaobjekt das Standardbild für dieses sammlungsobjekt.
    Standardbild kann den Pfad zum Bild enthalten, muss aber nicht.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="verknüpftesObjekt" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>
  	Ein mit dieser Person verknüpftes Objekt aus den Sammlungen. Es handelt 
  	sich bei der Nummer um eine ObjId, die auf einen Sammlungsobjektdatensatz 
  	referziert.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>


 <!-- CLASS 3 Sammlungsobjekte -->

<xsd:element name="sammlungsobjekt">
 <xsd:annotation>
  <xsd:documentation> Container für M+Datensätze. Das Attribut "objId" enthält
    einen
    eindeutigen Index für jeden Datensatz in M+.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:sequence>
   <xsd:element ref="abbildungen" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="aktuellerStandort" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="allgAngabeBeschriftung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="andereNr" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="anzahlTeile" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="ausstellung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="bearbDatum" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="bearbStand" minOccurs="0" maxOccurs="1"/>
    <!-- belichtungszeit (Foto) -->
    <!-- bemerkung (Foto; sollte wohl besser "fotoBemerkung" heißen) -->
   <xsd:element ref="bemerkungSammlung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="besetzung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="besitzart" minOccurs="0" maxOccurs="unbounded"/>
    <!-- blende (Foto?) -->
   <xsd:element ref="credits" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="datierung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="digitalisiert" minOccurs="0" maxOccurs="unbounded"/>
    <!-- dokumentation (Film und Audio; sollte wohl besser "audioFilmDokumentation" heißen) -->
   <xsd:element ref="dokumentation" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="erwerbDatum" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="erwerbNotiz" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="erwerbNr" minOccurs="0" maxOccurs="unbounded"/>
	<!-- alphabet error -->
   <xsd:element ref="erwerbungVon" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="erwerbungsart" minOccurs="0" maxOccurs="unbounded"/>
    <!-- farbe (Film) -->
   <xsd:element ref="farbe" minOccurs="0" maxOccurs="unbounded"/>
    <!-- filmtyp (Film) -->
    <!-- filter (Foto) -->
    <!-- form (Film, sollte wohl besser "filmForm" heißen) -->
   <xsd:element ref="form" minOccurs="0" maxOccurs="1"/>
    <!-- format (Audio) -->
   <xsd:element ref="format" minOccurs="0" maxOccurs="unbounded"/>

    <!-- geogrBezug (neu) statt geoBezug (alt) -->
   <xsd:element ref="geogrBezug" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="handlingVerpackungTransport" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="identNr" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="ikonographischeBeschreibung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="ikonographischeKurzbeschreibun" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="inhalt" minOccurs="0" maxOccurs="unbounded"/>
    <!-- instrumente (Audio) -->
   <xsd:element ref="instrumente" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="inventarNotiz" minOccurs="0" maxOccurs="unbounded"/>
    <!-- kamera (Film) -->
    <!-- kameratyp (Film?) -->
    <!-- kategorieGenre (Audio, Film) -->
   <xsd:element ref="kategorieGenre" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="kABeleuchtung" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="kABemLeihfähigkeit" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="kALuftfeuchtigkeit" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="kATemperatur" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="konvolut" minOccurs="0" maxOccurs="unbounded"/>

   <xsd:element ref="kurzeBeschreibung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="lagerungSicherheitPräsentation" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="langeBeschreibung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="leihgeber" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="materialTechnik" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="maßangaben" minOccurs="0" maxOccurs="unbounded"/>
    <!-- xsd:element ref="multimedia" minOccurs="0" maxOccurs="unbounded" / -->
   <xsd:element ref="musikgattung" minOccurs="0" maxOccurs="1"/>
    <!-- nadelschliff (Audio) -->
   <xsd:element ref="nadelschliff" minOccurs="0" maxOccurs="1"/>
    <!-- objektiv (Foto?) -->
    <!-- strange alphabetical order -->
   <xsd:element ref="objStatus" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="objektgruppe" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="objekttyp" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="onlineBeschreibung" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="oov" minOccurs="0" maxOccurs="unbounded"/>
    <!-- personKörperschaft (doppelter Singualar) statt personenKörperschaften -->
   <xsd:element ref="personenKörperschaften" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="sachbegriff" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="sachbegriffHierarchisch" minOccurs="0" maxOccurs="unbounded"/>
    <!-- schnitt (Film) -->
   <xsd:element ref="standort" minOccurs="0" maxOccurs="unbounded"/>
    <!-- stativ (Foto?) -->
    <!-- stelleFilm (Film) -->
   <xsd:element ref="ständigerStandort" minOccurs="0" maxOccurs="1"/>
    <!-- SWD (neu) -->
   <xsd:element ref="swd" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="systematikArt" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="technischeBemerkung" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="textOriginal" minOccurs="0" maxOccurs="unbounded"/>
   <xsd:element ref="titel" minOccurs="0" maxOccurs="unbounded"/>
    <!-- ton (Film) -->
   <xsd:element ref="ton" minOccurs="0" maxOccurs="1"/>
    <!-- tvNorm (Film) -->
   <xsd:element ref="tvNorm" minOccurs="0" maxOccurs="1"/>
    <!-- veranstaltung (Film) -->
   <xsd:element ref="verantwortlich" minOccurs="1" maxOccurs="unbounded"/>
    <!-- verfügbareFormate (Film) -->
   <xsd:element ref="verfügbareFormate" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="verwaltendeInstitution" minOccurs="0" maxOccurs="1"/>
   <xsd:element ref="vorlage" minOccurs="0" maxOccurs="unbounded"/>
    <!-- verwendetesLicht (Foto) -->
    <!-- vorlage (Audio) -->
	<xsd:element ref="wGAusVorgaben" minOccurs="0" maxOccurs="1"/>
	<xsd:element ref="wGGruppe" minOccurs="0" maxOccurs="1"/>
	<xsd:element ref="wGRestzeitGh" minOccurs="0" maxOccurs="1"/>
	<xsd:element ref="wGStänderung" minOccurs="0" maxOccurs="1"/>
	<xsd:element ref="wGZustand" minOccurs="0" maxOccurs="1"/>
    <!-- zielgruppe (Film) -->
   <xsd:element ref="zielgruppe" minOccurs="0" maxOccurs="1"/>
    <!-- zusatzgeräte (Film?) -->
   </xsd:sequence>
  <xsd:attribute name="exportdatum" type="xsd:dateTime"/>
  <xsd:attribute name="objId" type="xsd:integer"/>
  </xsd:complexType>
</xsd:element>

 <!-- main level -->
<xsd:element name="abbildungen" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Information über Abbildung zum Gegenstand. SN: z.B. "Foto
    auf
    Karteikarte", "Skizze auf Karteikare", "Zeichnung auf Karteikarte", "Digitale
    Abbildung".
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="allgAngabeBeschriftung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>missing.</xsd:documentation>
  </xsd:annotation>
</xsd:element>
<xsd:element name="aktuellerStandort" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Gegenwärtiger Standort innerhalb der Institution. SN:
    Eine
    Ortseingabe erfasst mehrere Elemente, z.B. "Depot/Studiensammlung (Süd- und
    Südostasien) / Regal: 1 , F: 4".
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="andereNr">
 <xsd:annotation>
  <xsd:documentation> Dieses Element erfasst bekannte ältere Inventarnummern
    bzw. bekannte
    andere Nummern (z.B. Saal-, Katalog- oder Sammlernummern). Das Attribut "art"
    kennzeichnet die Art der anderen Nummer: z.B. "Alte Ident. Nr",
    "Grabungsnummer".
    ("art" entspricht "Bezeichnung" im GUI von M+).
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="bemerkung" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="anzahlTeile" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Anzahl aller zugehörigen Teile eines Objektes.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="bearbStand" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>missing.</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="datierung">
 <xsd:annotation>
  <xsd:documentation> Bei Sammlungsobjekten: Ein mit dem Objekt assoziertes
    Datum. Bei PersonenKörperschaften: Näher bestimmtes Datum oder Zeitspanne, die mit der
    Person/Körperschaft zusammenhängen. Das Attribut "art" kennzeichnet die Art der
    Datierung: "Aufnahme" für das Datum einer Aufnahme, "Publikation" für das Datum der
    Veröffentlichung etc. Das Attribut "bemerkung" (nur bei Sammlungsobjekten)
    enthält
    weitere Informationen zu dieser Datierung.
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="bemerkung" type="xsd:string"/>
    <xsd:attribute name="bisJahr" type="xsd:string"/>
    <xsd:attribute name="bisMonat" type="xsd:string"/>
    <xsd:attribute name="bisTag" type="xsd:string"/>
    <xsd:attribute name="vonJahr" type="xsd:string"/>
    <xsd:attribute name="vonMonat" type="xsd:string"/>
    <xsd:attribute name="vonTag" type="xsd:string"/>
    <xsd:attribute name="sort" type="xsd:integer"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="bemerkungSammlung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Eine Bemerkung zum Objekt. </xsd:documentation>
  </xsd:annotation>
</xsd:element>

 <!-- Zusatz: Audio -->
<xsd:element name="besetzung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>
    Instrumente, die mit dem Objekt assoziiert werden, z.B.: Violine.
    <!-- Vorschlag: Besetzung wie bei Dismarc, also "solo, vokal" vs. genauere Instrumentenangabe -->
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="bearbDatum" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Bei Sammlungsobjekten, Multimediaobjekten und bei
    PersonenKörperschaften: Zeitpunkt der letzten Bearbeitung des Datensatz
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="ausstellung">
	<xsd:annotation>
	<xsd:documentation>
		dokumentierte Ausstellung, bei denen das Objekt gezeigt wurde
	</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="entscheid" type="xsd:string"/>
				<xsd:attribute name="datumVon" type="xsd:string"/>
				<xsd:attribute name="datumBis" type="xsd:string"/>
                <xsd:attribute name="internExtern" type="xsd:string"/>
                <xsd:attribute name="jahr" type="xsd:string"/>
                <xsd:attribute name="katalogNr" type="xsd:string"/>
                <xsd:attribute name="ort" type="xsd:string"/>
                <xsd:attribute name="sektion" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:element>


<xsd:element name="besitzart" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			Art des Besitzes. SN: z.B. "Eigentum".
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="credits" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>missing.</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="erwerbDatum" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Datum des Erwerbs. SN: notiert in Jahreszahl oder genaue
    Datumsangabe.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="erwerbNotiz">
	<xsd:annotation>
		<xsd:documentation>
		Anmerkung, die mit dem Erwerb des Gegenstandes in Zusammenhang stehen. 
		
		SN: z.B. "Reise", "Tausch", "Spendenbescheinigung".
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="typ" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:element>

<xsd:element name="erwerbNr" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>Todo</xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="erwerbungsart" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Art der Aneignung. SN: z.B. "Ankauf", "Schenkung".
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="erwerbungVon" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Person/Körperschaft, von der der Gegenstand erworben
    wurde.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="form" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Form von Filmen (Zusatz 1). </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="identNr">
 <xsd:annotation>
  <xsd:documentation> Eine Nummer, die das Objekt eindeutig identifiziert.
   </xsd:documentation>
  </xsd:annotation>

 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
    <xsd:attribute name="art" type="xsd:string"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>


 <!-- Zusatz: Audio -->
<xsd:element name="digitalisiert" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Angabe zum den Stand der Digitalisierung des Objekts mit
    den zwei
    Möglichkeiten 'Ja' oder 'Nein'.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="dokumentation" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Informationen zu zusaetzlichen Dokumentation des Objekts.
    Oft
    Ja/Nein, wenn weitere Dokumentation vorhanden oder nicht. Kann auch Hinweise auf den
    Ort der weiteren Dokumentation enthalten.
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="geogrBezug">
 <xsd:annotation>
  <xsd:documentation> Bei Sammlungsobjekten: Angabe zu geografischen Bezügen,
    die mit dem
    Objekt assoziiert sind. Bei PersonenKörperschaften: näher bestimmter Ort, welcher
    der Person oder Körperschaft angehört. Jeder einzelne Satz der darauf
    bezogenen
    Daten innerhalb dieses Komplexes verweist auf eine Ortsangabe. Attribute: -art
    (alte
    Bezeichnung fuer Funktion, in M+Ortstyp) -bezeichnung: Typ des Ortes, z.B. Land
    -funktion: Ortsrolle (neu) -kommentar (nur bei Sammlungsobjekten).
   </xsd:documentation>
  </xsd:annotation>
 <xsd:complexType>
  <xsd:simpleContent>
   <xsd:extension base="xsd:string">
     <!-- names of attribute changed 8.8.07 -->
    <xsd:attribute name="art" type="xsd:string"/>
    <xsd:attribute name="bezeichnung" type="xsd:string"/>
    <xsd:attribute name="funktion" type="xsd:string"/>
    <xsd:attribute name="kommentar" type="xsd:string"/>
    <xsd:attribute name="sort" type="xsd:integer"/>
    </xsd:extension>
   </xsd:simpleContent>
  </xsd:complexType>
</xsd:element>

<xsd:element name="handlingVerpackungTransport" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
  		Information über Auflagen des Handlings, der Verpackung und des Transports.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

<xsd:element name="ikonographischeBeschreibung" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> ikonographische Beschreibung. </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="ikonographischeKurzbeschreibun" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> kurze Beschreibung der Ikonographie. </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="inhalt" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>
    Freitext zur Angabe zum Inhalt eines Objekts. Enthält bspw. genaue
    Beschreibung zu beiden Seiten einer Schallplatte und Interpretennamen.
    <!-- Kann unter
     Umständen gehandhabt werden wie bei Dismarc mit
     'contributor'-Feld für den Interpreten -->
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="instrumente" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation>
    Freitext zur Angabe der in der Aufnahme vorkommenden Instrumente.
    veraltetes Feld bei Film/Video.
    <!-- Kann unter
     Umständen gehandhabt werden wie bei Dismarc mit
     'contributor'-Feld für den Interpreten -->
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="inventarNotiz" type="xsd:string">
 <xsd:annotation>
  <xsd:documentation> Notiz zur Bestandsaufnahme des Gegenstandes. SN: z.B.
    "Inventarbuch
    Schneider S. 101-102".
   </xsd:documentation>
  </xsd:annotation>
</xsd:element>

<xsd:element name="kategorieGenre" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>
			bestimmt die Kategorie und das Genre näher, bspw. Frauenfilm, Tierfilm etc.
		</xsd:documentation>
	</xsd:annotation>
</xsd:element>

